/* pages/VideoPlay/index.wxss */
#myVideo{
  width: 100%;
  height: 425rpx;
}
.more-mv{
  padding: 0 10rpx;
}
.more-mv .head{
  padding: 10rpx;
}
.detail{
  padding: 10rpx;
  font-size: 20rpx;
}
.detail .img{
  width: 100%;
  height: 128rpx;
  background: #000;
  margin-bottom: 8rpx;
}
.detail .img image{
    width: 100%;
    display: block;
    height: 100%;
}
.detail .title{
  height: 30rpx;
  line-height: 30rpx;
  overflow: hidden;
  margin-bottom: 10rpx;
}